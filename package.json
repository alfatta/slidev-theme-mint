{
  "name": "slidev-theme-mint",
  "version": "0.1.0",
  "author": "alfatta",
  "type": "module",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=18.0.0",
    "slidev": ">=0.19.3"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^0.49.12",
    "prism-theme-vars": "^0.2.5"
  },
  "devDependencies": {
    "@slidev/cli": "^0.49.12",
    "playwright-chromium": "^1.45.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alfatta/slidev-theme-mint.git"
  },
  "bugs": {
    "url": "https://github.com/alfatta/slidev-theme-mint/issues"
  },
  "homepage": "https://github.com/alfatta/slidev-theme-mint#readme",
  "slidev": {
    "colorSchema": "light",
    "highlighter": "all",
    "defaults": {
      "fonts": {
        "sans": "IBM Plex Sans",
        "weights": "200,300,400,500,600,700,800",
        "mono": "Fira Code"
      }
    }
  }
}